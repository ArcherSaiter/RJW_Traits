	<GeneDef ParentName="TDiseaseBase" MayRequire="vegapnk.rjw.genes">
        <defName>RJW_Unleashed_Bimbofier</defName>
        <label>Bimbofier</label>
        <description>Carriers of this gene slowly change their partners into dumb, but stereotypically attractive versions.</description>
        <iconPath>Genes/Icons/Playful_sadist</iconPath>
        <biostatCpx>3</biostatCpx>
        <biostatMet>0</biostatMet>
        <displayOrderInCategory>50</displayOrderInCategory>
        <modExtensions>
            <li Class="RJW_Genes.ImmunityAgainstGenesExtension">
                <givesImmunityAgainst>
                    <li>RJW_Unleashed_Bimbofier</li>
                </givesImmunityAgainst>
            </li>
            <li Class="RJW_Genes.HediffIncreaseOnSexExtension">
                <hediffDef>RJW_Unleashed_Bimbofication</hediffDef>
                <canCreateHediff>true</canCreateHediff>
                <severityIncrease>0.2</severityIncrease>
                <applicationChance>1.00</applicationChance>
                <applicableForWomen>true</applicableForWomen>
                <applicableForMen>true</applicableForMen>
                <requiresPenetrativeSex>false</requiresPenetrativeSex>
            </li>
            <li Class="RJW_Genes.GeneAlteringExtension">
                <majorGenes>
					<li>MeleeDamage_Weak</li>
					<li>rjw_genes_big_breasts</li>
					<li>rjw_genes_lifeforce</li>
					<li>rjw_genes_seduce</li>
                </majorGenes>
                <minorGenes>
                    <li>RJW_Traits_Bimbo_Gene</li>
					<li>rjw_genes_big_breasts</li>
                    <li>Beard_NoBeardOnly</li>
					<li>Body_Standard</li>
					<li>Hair_Blonde</li>
					<li>Hair_Grayless</li>
					<li>Mood_Optimist</li>
					<li>rjw_genes_size_blinded</li>
                </minorGenes>
                <minorApplicationChance>0.75</minorApplicationChance>
                <majorApplicationChance>0.35</majorApplicationChance>
            </li>
        </modExtensions>
    </GeneDef>
	
