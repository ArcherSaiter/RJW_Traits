	<GeneDef ParentName="TDiseaseBase" MayRequire="vegapnk.rjw.genes">
        <defName>RJW_Unleashed_InsatiableInfective</defName>
        <label>Infective insatiability</label>
        <description>Carriers of this gene are unnaturally lustful, and tend to spread their lust as a disease.</description>
        <iconPath>Genes/Icons/Insatiable</iconPath>
        <biostatCpx>1</biostatCpx>
        <biostatMet>2</biostatMet>
        <displayOrderInCategory>60</displayOrderInCategory>
		<forcedTraits>
            <li>
                <def>RJW_Traits_Insatiable</def>
            </li>
        </forcedTraits>
        <modExtensions>
            <li Class="RJW_Genes.ImmunityAgainstGenesExtension">
                <givesImmunityAgainst>
                    <li>RJW_Unleashed_InsatiableInfective</li>
                </givesImmunityAgainst>
            </li>
			<li Class="RJW_Genes.GeneticInfectorExtension">
                <infectionChance>0.05</infectionChance>
                <infectionGenes>
                    <li>RJW_Unleashed_InsatiableInfective</li>
                </infectionGenes>
            </li>
        </modExtensions>
		<exclusionTags>
			<li>Sex_Thirst</li>
